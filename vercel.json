{
  "version": 2,
  "functions": {
    "pages/api/parse-job.js": {
      "memory": 3008,
      "maxDuration": 300
    },
    "pages/api/analyze-call.js": {
      "memory": 3008,
      "maxDuration": 300
    }
  },
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.crackedhire.com"
        }
      ],
      "destination": "https://crackedhire.com/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/hire",
      "permanent": true
    }
  ]
}